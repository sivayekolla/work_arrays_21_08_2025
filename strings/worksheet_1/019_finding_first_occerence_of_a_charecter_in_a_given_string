// finding first occurence of a charecter in a given string
#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main()
{
     char arr[100];char ch;
     int count=0;//using count to know the position of perticulor charecter
     fgets(arr,100,stdin);
     arr[strcspn(arr,"\n")]='\0';//replacing  "\n"  with '\0'
     scanf("%c",&ch);
     char *ptr=arr;
     while( *ptr != '\0' )//traversing pointer until break 
     {
          count++;
          if(*ptr == ch)
          {
               break;
          }
          ptr++;
     }
     printf("%d",count);

}